{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "ordered-imports": false,
    "member-ordering": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "only-arrow-functions": false,
    "no-string-literal": false,
    "jsx-no-lambda": false
  }
}
